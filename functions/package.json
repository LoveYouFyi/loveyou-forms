{
  "name": "loveyouforms",
  "description": "Cloud Functions for Firebase",
  "## Notes for Scripts ####################################################": {
    "exportData": "you may never need this",
    "importData": "will only need to use this once to import starter database"
  },
  "scripts": {
    "exportData": "node -e 'require(\"./export-import\").firestoreExport()'",
    "importData": "node -e 'require(\"./export-import\").firestoreImport()'",
    "serve": "firebase serve --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "8"
  },
  "## Notes for Dependencies ###############################################": {
    "firestore-export-import": "required once to import starter database",
    "fs": "only needed if you want to run the script exportData which saves to file"
  },
  "dependencies": {
    "firebase-admin": "^8.6.0",
    "firebase-functions": "^3.3.0",
    "firestore-export-import": "^0.5.0",
    "fs": "0.0.1-security",
    "googleapis": "^36.0.0",
    "moment-timezone": "^0.5.28"
  },
  "devDependencies": {
    "firebase-functions-test": "^0.1.6"
  },
  "private": true
}
